<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henktech.maskup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/maskup_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/maskup_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.MainActivity"
            android:label="Mask Up">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.HomeActivity"
            android:label="Home" />
        <activity
            android:name=".activities.FlatActivity"
            android:label="Flat" />

        <receiver android:name=".publishers.NotificationPublisher" />
    </application>
</manifest>